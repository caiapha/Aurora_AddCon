	<!-- standard -->
	<element name="Primordial" type="Language" source="Player’s Handbook" id="ID_LANGUAGE_PRIMORDIAL">
		<supports>Standard</supports>
		<description>
			<p>Typically spoken by Elementals and similar creaturs.</p>
		</description>
		<setters>
			<set name="standard">true</set>
			<set name="speakers">Elemental and similar creaturs</set>
			<set name="script">Dwarvish</set>
		</setters>
	</element>
